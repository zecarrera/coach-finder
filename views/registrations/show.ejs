<!DOCTYPE html>
<html>

<head>
    <title>CoachFinder</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
</head>

<body>
    <div class="container">
        <div class="row">
            <div class="col-md-3">
                <p class="lead">Topic: <%= topic.topicTitle %></p>
                <div class="list-group">
                    <% if (participants.length > 0) { %>
                        <ol>
                            <% participants.forEach(function(participant){ %>
                                <div class="row">
                                    <div class="col-md-12">
                                        <li>
                                            <%= participant.applicantUsername %>
                                        </li>
                                    </div>
                                </div>
                                <% }); %>
                            </ol>
                    <% } else{ %>
                        <p>No participants registered on this topic.</p>
                    <% } %>
                </div>
            </div>

        </div>
    </div>
</body>
</html>
